{
  "name": "e-brochure-admin-panel",
  "version": "1.1.3",
  "description": "Admin Panel of Utec E-Brochure",
  "main": "index.js",
  "scripts": {
    "start": "REACT_APP_ENV=dev react-scripts start",
    "build-dev": "react-csp dev && REACT_APP_ENV=dev react-scripts build",
    "build-uat": "react-csp uat && REACT_APP_ENV=uat react-scripts build",
    "build-prod": "react-csp prod && REACT_APP_ENV=prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.4.0",
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@react-pdf/renderer": "^3.4.2",
    "@reduxjs/toolkit": "^1.9.7",
    "aws-amplify": "^5.3.11",
    "aws-amplify-react": "^5.1.43",
    "axios": "^1.6.2",
    "bootstrap": "^4.6.2",
    "chart.js": "2.9.4",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "jspdf": "^2.5.1",
    "moment": "2.29.4",
    "nouislider": "15.4.0",
    "pdf-image": "2.0.0",
    "pdfjs-dist": "2.6.347",
    "react": ">=16.0.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-chartjs-2": "2.11.2",
    "react-copy-to-clipboard": "5.1.0",
    "react-cropper": "^2.3.3",
    "react-csp": "^1.1.3",
    "react-datetime": "3.2.0",
    "react-dom": ">=16.0.0",
    "react-dropzone": "^14.2.3",
    "react-easy-crop": "^5.0.6",
    "react-hot-toast": "^2.4.1",
    "react-pdf": "^7.7.1",
    "react-pdf-to-image": "^1.0.2",
    "react-player": "^2.14.1",
    "react-quill": "^2.0.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "6.11.1",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.0",
    "react-toggle-button": "^2.2.0",
    "react-tooltip": "^4.2.21",
    "react-window": "^1.8.10",
    "reactstrap": "8.10.0",
    "redux-persist": "^6.0.0",
    "sass": "1.62.1",
    "validator": "^13.11.0",
    "xlsx": "^0.18.5",
    "yet-another-react-lightbox": "^3.17.2"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "8.0.3",
    "typescript": "5.0.4"
  },
  "overrides": {
    "svgo": "3.0.2",
    "fsevents": "2.3.2",
    "chokidar": "3.5.3"
  },
  "devDependencies": {
    "ajv": "^8.16.0",
    "worker-loader": "^3.0.8"
  }
}
